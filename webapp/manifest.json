{
    "_version": "1.65.0",
    "sap.app":{
        "id": "infy.com",
        "type": "application",
        "ach": "IFY-DMO-APP",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "dataSources": {
            
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}"

    },
    "sap.ui": {
        "technology": "UI5",
        "deviceTypes":{
            "desktop": true,
            "phone": true,
            "tablet": true
        },
        "fullWidth": true
    },
    "sap.ui5": {
        "flexEnabled": false,
        "dependencies": {
            "minUI5Version": "1.127.2",
            "libs": {
                "sap.m": {},
                "sap.ui.core":{}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "rootView":{
            "id": "idRootView",
            "viewName": "infy.com.view.App",
            "type": "XML"
        },
        "routing": {
            "config":{
                "routerClass": "sap.m.routing.Router",
                "viewPath": "infy.com.view",
                "viewType": "XML",
                "controlId": "idAppCon",
                "async": true
            },
            "routes":[
                {
                    "pattern": "",
                    "name": "spiderman",
                    "target":["joe", "biden"]
                },{
                    "pattern": "fruit/{fruitId}",
                    "name": "superman",
                    "target":["joe", "modi"]
                },{
                    "pattern": "supplier/{suppId}",
                    "name": "ironman",
                    "target":["joe", "putin"]
                }
            ],
            "targets": {
                "joe":{
                    "viewId": "idView1",
                    "viewName": "View1",
                    "controlAggregation": "masterPages"
                },
                "biden":{
                    "viewId": "idEmpty",
                    "viewName": "Empty",
                    "controlAggregation": "detailPages"
                },
                "modi":{
                    "viewId": "idView2",
                    "viewName": "View2",
                    "controlAggregation": "detailPages"
                },
                "putin":{
                    "viewId": "idView3",
                    "viewName": "View3",
                    "controlAggregation": "detailPages"
                }
            }
        },
        "models": {
            "":{
                "type": "sap.ui.model.json.JSONModel",
                "preload": false,
                "uri": "mockdata/fruits.json"
            },
            "i18n":{
                "type": "sap.ui.model.resource.ResourceModel",
                "preload": true,
                "settings": {
                    "bundleUrl": "i18n/i18n.properties"
                }
            }
        }
    }
}